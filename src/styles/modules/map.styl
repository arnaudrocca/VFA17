.mapContainer
	position absolute
	width 100vw
	height 100vh
	will-change transform
	transition all 1s $easeOutExpo

	.map
		position absolute
		width 100%
		top -25%
		overflow visible
		will-change transform
		transition transform 1.5s $easeOutExpo

		.mapItems
			position absolute
			width 100%
			top 0

			.mapItem
				position absolute
