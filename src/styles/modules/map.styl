.mapContainer
	position absolute
	display flex
	flex-direction column
	justify-content center
	align-items center
	width 100vw
	height 100vh
	top 0
	left 0
	transition transform-origin 1s $easeOutExpo, transform 1s $easeOutExpo

	.map
		position relative
		width 100%
		height auto
		overflow visible
		transition transform 1s $easeOutExpo

		.mapItems
			position absolute
			width 100%
			height 100%
			top 0
			left 0

			.mapItemContainer
				position absolute
				width 100%
				height 100%

				.mapItem
					position absolute

