@keyframes pulse
    0%
        transform scale(0.85)

    50%
        transform scale(1.1)

    100%
       transform scale(0.85)

@keyframes rotate
   from
        transform rotate(0deg)

   to
        transform rotate(360deg)

@keyframes blink
    0%
        opacity .4

    50%
        opacity 1

    100%
        opacity .4
